            <TextField fx:id="dest" onMouseClicked="#handleAfficherLeCheminEnCourant" onMouseReleased="#handleAfficherLeCheminEnCourant" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="dist" onMouseClicked="#handleAfficherLeCheminEnCourant" onMouseReleased="#handleAfficherLeCheminEnCourant" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button mnemonicParsing="false" text="MODIFIER" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="INSERER" GridPane.columnIndex="3" GridPane.rowIndex="4" />
